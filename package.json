{
  "name": "ideanick",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "b": "pnpm --filter @ideanick/backend",
    "w": "pnpm --filter @ideanick/webapp",
    "sh": "pnpm --filter @ideanick/shared",
    "dev": "pnpm -r --parallel dev",
    "test": "pnpm -r test",
    "types": "concurrently 'pnpm b types' 'pnpm w types' 'pnpm sh types'",
    "lint": "concurrently 'pnpm b lint' 'pnpm w lint' 'pnpm sh lint'",
    "prettify": "pnpm -r --parallel prettify",
    "prepare": "husky && husky install"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/cors": "^2.8.18",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^4.4.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "sass": "^1.88.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.3.0",
    "tsx": "^4.19.4",
    "typescript": "~5.8.3",
    "typescript-transform-paths": "^3.5.5"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.75.7",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "include-media": "^2.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "reset-css": "^5.0.2",
    "usehooks-ts": "2.9.1"
  }
}
